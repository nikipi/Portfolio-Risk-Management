    "## Random Walk"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 88,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Set the simulation parameters\n",
    "mu = np.mean(uso)\n",
    "vol = np.std(uso)\n",
    "T = 252\n",
    "S0 = 10\n",
    "\n",
    "# Add one to the random returns\n",
    "rand_rets = np.random.normal(mu,vol,T)+ 1\n",
    "\n",
    "# Forecasted random walk\n",
    "forecasted_values = S0*(rand_rets.cumprod())\n",
    "\n",
    "# Plot the random walk\n",
    "plt.plot(range(0, T), forecasted_values)\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Monte Carlo Simulations"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 89,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "-0.03581845972425982"
      ]
     },
     "execution_count": 89,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "sim_return=[]\n",
    "for i in range(100):\n",
    "    rand_rets=np.random.normal(mu,vol,T)\n",
    "    sim_return.append(rand_rets)\n",
    "\n",
    "var_95=np.percentile(sim_return,5)\n",
